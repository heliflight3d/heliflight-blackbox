{
  "name": "HeliFlight Blackbox",
  "description": "Interactive flight log viewer for HeliFlight3D",
  "version": "3.6.0",
  "manifest_version": 2,
  "default_locale": "en",
  "app": {
    "background": {
      "scripts": ["chromeBackground.js"]
    }
  },
  "permissions": [
       {"fileSystem": ["write"]},
       "storage"
   ],
  "icons": { "128": "images/bf_icon_128.png" }
}
